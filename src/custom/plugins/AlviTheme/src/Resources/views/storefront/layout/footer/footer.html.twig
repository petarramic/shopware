{% block layout_footer_inner_container %}
  <div class="px-2 py-5 p-md-5 d-flex flex-column mx-md-5 justify-items-center">
    <div class="container d-flex flex-column gap-5">
      {% block layout_footer_logo %}
        <div class="col-12 col-lg-auto d-flex justify-content-center">
          {% sw_include '@Storefront/storefront/layout/header/logo.html.twig' %}
        </div>
      {% endblock %}
      {% block layout_footer_navigation %}
        <div id="footerColumns" class="row footer-columns gy-5" data-collapse-footer-columns="true" role="list">
          {% block layout_footer_newsletter %}
            <div class="col-md-3 d-flex flex-column gap-3">
              <h1 className="footer-column-headline">Newsletter</h1>
              {% block layout_footer_newsletter_input %}
                <input type="email" name="newsletter" class="form-control" autocomplete="off" autocapitalize="off" placeholder="Deine E-Mail-Adresse" aria-label="Deine E-Mail-Adresse" />
              {% endblock %}
              {% block layout_footer_newsletter_submit %}
                <div>
                  <button type="submit" class="btn btn-primary footer-btn">Anmelden</button>
                </div>
              {% endblock %}

              {% block layout_footer_newsletter_agreement %}
                <div class="d-flex gap-3">
                  <input type="checkbox" class="form-check-input" id="newsletter-agreement" name="newsletter-agreement" />
                  <label class="form-label" for="newsletter-agreement">Lorem ipsum dolor sit amet consectetur adipisicing elit. Quo amet.</label>
                </div>
              {% endblock %}
            </div>
          {% endblock %}
          {% block layout_footer_service %}
            <div class="col-md-3 d-flex flex-column gap-3">
              <h1 className="footer-column-headline">Service</h1>
              {% block layout_footer_service_list %}
                <div class="d-flex flex-column gap-1">
                  <div>Größenrechner</div>
                  <div>Händlersuche</div>
                  <div>Internationaleg Gößentabelle</div>
                  <div>Versandbedingungen</div>
                  <div>Zahlung</div>
                  <div>Reklamation</div>
                </div>
              {% endblock %}
            </div>
          {% endblock %}
          {% block layout_footer_legal %}
            <div class="col-md-3 d-flex flex-column gap-3">
              <h1 className="footer-column-headline">Rechtliches</h1>
              {% block layout_footer_legal_list %}
                <div class="d-flex flex-column gap-1">
                  <div>AGB</div>
                  <div>Datenschutz</div>
                  <div>Impressum</div>
                  <div>Widerrufsrecht</div>
                </div>
              {% endblock %}
            </div>
          {% endblock %}
          {% block layout_footer_contact %}
            <div class="col-md-3 d-flex flex-column gap-3">
              <h1 className="footer-column-headline">Kontakt</h1>
              {% block layout_footer_contact_info %}
                <div class="d-flex flex-column gap-3">
                  <div class="d-flex flex-column">
                    <div>ALVI GMBH</div>
                    <div>Pfennigbreite 44</div>
                    <div>DE-37671 Höxter</div>
                  </div>
                  <a href="#">webshop@alvi.de</a>
                </div>
              {% endblock %}
            </div>
          {% endblock %}
          {% block layout_footer_payments %}
            <div class="col-md-3 d-flex flex-column gap-3">
              <h1 className="footer-column-headline">Zahlungsmittel</h1>
              {% block layout_footer_payments_list %}
                <div class="row g-3">
                  <div class="col-4">
                    <div class="payment-method">
                    </div>
                  </div>
                  <div class="col-4">
                    <div class="payment-method">
                    </div>
                  </div>
                  <div class="col-4">
                    <div class="payment-method">
                    </div>
                  </div>
                  <div class="col-4">
                    <div class="payment-method">
                    </div>
                  </div>
                  <div class="col-4">
                    <div class="payment-method">
                    </div>
                  </div>
                </div>
              {% endblock %}
            </div>
          {% endblock %}
          {% block layout_footer_social_media %}
            <div class="col-md-9 d-flex flex-column gap-3 ps-md-5">
              <h1 className="footer-column-headline">Social Media</h1>
              {% block layout_footer_ocial_media_list %}
                <div class="d-flex gap-3">
